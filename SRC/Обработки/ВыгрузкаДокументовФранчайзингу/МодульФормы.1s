							Если (РезультатЗапроса.СуммаУпрПриход > 0) Или (РезультатЗапроса.СуммаУпрРасход > 0) Тогда
								Организация = РезультатЗапроса.ЮрЛицо;
							КонецЕсли;
						КонецЕсли;
					КонецЕсли;				
				КонецЕсли;		
					ВходящиеДанные = СоздатьОбъект("СписокЗначений");
					ВходящиеДанные.ДобавитьЗначение(Организация,"Организация");
					Если ПустоеЗначение(Организация) = 0 Тогда
						КлючВыгружаемыхДанных = КлючВыгружаемыхДанных + Организация;
				   	КонецЕсли;
