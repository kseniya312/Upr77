			НеПереноситьНомерИДатуСертификата = ПустоеЗначение (Параметры.НомерИДатаСертификата);
			
			Если НеПереноситьСерийныйНомер = 0 Тогда
				Если Источник.Свойство.Владелец = Параметры.СерийныйНомер Тогда
					Свойство = Источник.Свойство.Наименование;
					ВходящиеДанные = СоздатьОбъект("СписокЗначений");
					ВходящиеДанные.ДобавитьЗначение(Свойство, "СерийныйНомер");
				Иначе
					НеПереноситьСерийныйНомер = 1;
				КонецЕсли;	
			КонецЕсли;
			
			Если НеПереноситьНомерИДатуСертификата = 0 Тогда
				Если Источник.Свойство.Владелец = Параметры.НомерИДатаСертификата Тогда
