Функция ВыгрузитьТелефоны(
			ИсходящиеДанные = "", ВходящиеДанные = "", Источник = "", Приемник = "", ТипПриемника = "", Значение = "", Выражение = "",
			Пусто = "", ВыборкаДанных = "", Объект = "", КоллекцияОбъектов = "", ОбъектКоллекции = "", ИмяПКО = "", ИмяПКОВидСубконто = "",
			НомерПКО = "", НомерПКС = "", Правило = "", КлючВыгружаемыхДанных = "", НеЗапоминатьВыгруженные = "", НеЗамещатьОбъектПриЗагрузке = "",
			ВсеОбъектыВыгружены = "", ТолькоПолучитьУзелСсылки = "", РежимЗаписи = "", РежимПроведения = "", СтандартнаяОбработка = "",
			НеЗамещать = "", НеОчищать = "", УзелСсылки = "", УзелКоллекцииОбъектов = "", УзелОбъектаКоллекции = "", УзелСвойства = "",
			ВидСубконто = "", Субконто = "", Отказ = "")


	
	Если Источник.Вид() = "СвоиЮрЛица" Тогда
		Вид = "ТелефоныОрганизации";
		Справочник = "Организации";
		Представление = Источник.Телефоны;
	ИначеЕсли Источник.Вид() = "Контрагенты" Тогда
		Вид = "ТелефоныКонтрагента";
		Справочник = "Контрагенты";
		Представление = Источник.ЮрФизЛицо.Телефоны;
	Иначе
		Вид = "ТелефоныФизЛица";
		Справочник = "ФизическиеЛица";
		Представление = Источник.Телефоны;	
	КонецЕсли;
		 
	Если ПустаяСтрока(Представление) = 0 Тогда
		 СтрокаПоиска = СокрЛП(Представление);
		 Если ПустоеЗначение(Параметры.РазделительТелефонов) = 1 Тогда
		 	Разделитель = ",";
		 Иначе
			Разделитель = СокрЛП(Параметры.РазделительТелефонов);
		 КонецЕсли;
		 	 
		 ПозицияРазделителя = Найти(СтрокаПоиска,Разделитель); 
		 Если ПозицияРазделителя = 0 Тогда
			ИсходящиеДанные = СоздатьОбъект("СписокЗначений");
			ИсходящиеДанные.ДобавитьЗначение(Источник, "Объект");
			ИсходящиеДанные.ДобавитьЗначение("Телефон",  "Тип");
			ИсходящиеДанные.ДобавитьЗначение(Вид,  "Вид");
			ИсходящиеДанные.ДобавитьЗначение(СтрокаПоиска,  "Представление");
			ИсходящиеДанные.ДобавитьЗначение(Справочник,"Справочник");
			ВыгрузитьПоПравилу(,, ИсходящиеДанные,, "КонтактнаяИнформация");
		  Иначе
			 ПорядковыйНомер = 0;
			 Пока ПозицияРазделителя > 0 Цикл
				Телефон = Лев(СтрокаПоиска,ПозицияРазделителя-1);
			 	СтрокаПоиска = Прав(СтрокаПоиска,СтрДлина(СтрокаПоиска) - ПозицияРазделителя);
				ПорядковыйНомер = ПорядковыйНомер + 1;
				
				Если Источник.Вид() = "СвоиЮрЛица" Тогда
					Вид = "Телефон организации " + Строка(ПорядковыйНомер);
				ИначеЕсли Источник.Вид() = "Контрагенты" Тогда
					Вид = "Телефон контрагента " + Строка(ПорядковыйНомер); 
				Иначе
					Вид = "Телефон физ. лица " + Строка(ПорядковыйНомер);
				КонецЕсли;
	
				ИсходящиеДанные = СоздатьОбъект("СписокЗначений");
				ИсходящиеДанные.ДобавитьЗначение(Источник, "Объект");
				ИсходящиеДанные.ДобавитьЗначение("Телефон",  "Тип");
				ИсходящиеДанные.ДобавитьЗначение(Вид,  "Вид");
				ИсходящиеДанные.ДобавитьЗначение(Телефон,  "Представление");
				ИсходящиеДанные.ДобавитьЗначение(Справочник,"Справочник");
				ВыгрузитьПоПравилу(,, ИсходящиеДанные,, "КонтактнаяИнформация");
				ПозицияРазделителя = Найти(СтрокаПоиска,Разделитель);
			КонецЦикла;
			Если Источник.Вид() = "СвоиЮрЛица" Тогда
				Вид = "ТелефоныОрганизации";
			ИначеЕсли Источник.Вид() = "Контрагенты" Тогда
				Вид = "ТелефоныКонтрагента";
			Иначе
				Вид = "ТелефоныФизЛица";
			КонецЕсли;
			ИсходящиеДанные = СоздатьОбъект("СписокЗначений");
			ИсходящиеДанные.ДобавитьЗначение(Источник, "Объект");
			ИсходящиеДанные.ДобавитьЗначение("Телефон",  "Тип");
			ИсходящиеДанные.ДобавитьЗначение(Вид,  "Вид");
			ИсходящиеДанные.ДобавитьЗначение(СтрокаПоиска,  "Представление");
			ИсходящиеДанные.ДобавитьЗначение(Справочник,"Справочник");
			ВыгрузитьПоПравилу(,, ИсходящиеДанные,, "КонтактнаяИнформация");
		 КонецЕсли;		 
	  КонецЕсли;


КонецФункции // ВыгрузитьТелефоны()

Функция ПроверитьЗаполнениеТабличнойЧасти(
			ИсходящиеДанные = "", ВходящиеДанные = "", Источник = "", Приемник = "", ТипПриемника = "", Значение = "", Выражение = "",
			Пусто = "", ВыборкаДанных = "", Объект = "", КоллекцияОбъектов = "", ОбъектКоллекции = "", ИмяПКО = "", ИмяПКОВидСубконто = "",
			НомерПКО = "", НомерПКС = "", Правило = "", КлючВыгружаемыхДанных = "", НеЗапоминатьВыгруженные = "", НеЗамещатьОбъектПриЗагрузке = "",
			ВсеОбъектыВыгружены = "", ТолькоПолучитьУзелСсылки = "", РежимЗаписи = "", РежимПроведения = "", СтандартнаяОбработка = "",
			НеЗамещать = "", НеОчищать = "", УзелСсылки = "", УзелКоллекцииОбъектов = "", УзелОбъектаКоллекции = "", УзелСвойства = "",
			ВидСубконто = "", Субконто = "", Отказ = "")


	СтрокаСообщения = "";
	
	Если ПустоеЗначение (ОбъектКоллекции.Номенклатура) = 1 Тогда
		Отказ = 1;
		СтрокаСообщения = "Не указана номенклатура. ";
	ИначеЕсли (ОбъектКоллекции.Номенклатура.ВидНоменклатуры = Перечисление.ВидыНоменклатуры.Услуга) Тогда
		 Отказ = 1;
		 СтрокаСообщения = "Номенклатура является услугой. ";
	ИначеЕсли  (ОбъектКоллекции.Номенклатура.ВидНоменклатуры = Перечисление.ВидыНоменклатуры.Работа) Тогда
		 Отказ = 1;
		 СтрокаСообщения = "Номенклатура является работой. "; 
	КонецЕсли;
	Если ОбъектКоллекции.Количество = 0 Тогда
		Отказ = 1;
		СтрокаСообщения = СтрокаСообщения + "Количество равно нулю. ";
	КонецЕсли;
	
	Если Отказ = 1 Тогда
		Сообщить (" В документе " + Источник + " строка табличной части " + ОбъектКоллекции.НомерСтроки +  " не выгружена! " + СтрокаСообщения ,".");	
	КонецЕсли;


КонецФункции // ПроверитьЗаполнениеТабличнойЧасти()

