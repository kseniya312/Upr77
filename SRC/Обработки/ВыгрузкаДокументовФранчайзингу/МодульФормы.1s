Функция ПКО_ПослеВыгрузки_ФизическиеЛица(
			Источник, ВходящиеДанные, ИсходящиеДанные, ИмяПКО, Приемник, УзелСсылки)


	Отказ = 0;


	Попытка


		ФИО     = Источник.ПолнНаименование;
		Поз1    = Найти(ФИО, " ");
		Фамилия = Лев(ФИО, Поз1);
		ИО      = Сред(ФИО, Поз1+1);
		Поз2    = Найти(ИО, " ");
		Имя     = Лев(ИО, Поз2);
		Отчество= Сред(ИО, Поз2+1);
		
		Если ПустаяСтрока(Фамилия) = 0 Тогда
			ИсходящиеДанные = СоздатьОбъект("СписокЗначений");
			ИсходящиеДанные.ДобавитьЗначение(Источник, "ФизЛицо");
			ИсходящиеДанные.ДобавитьЗначение(Фамилия,  "Фамилия");
			ИсходящиеДанные.ДобавитьЗначение(Имя,      "Имя");
			ИсходящиеДанные.ДобавитьЗначение(Отчество, "Отчество");
			ВыгрузитьПоПравилу(,, ИсходящиеДанные,, "ФИОФизЛиц");
		КонецЕсли;
		
		Если ПустаяСтрока(Источник.Телефоны) = 0 Тогда
			Если ПустоеЗначение (Параметры.РазделительТелефонов) = 1 Тогда
			ИсходящиеДанные = СоздатьОбъект("СписокЗначений");
			ИсходящиеДанные.ДобавитьЗначение(Источник, "Объект");
			ИсходящиеДанные.ДобавитьЗначение("Телефон", "Тип");
			ИсходящиеДанные.ДобавитьЗначение(Источник.Телефоны, "Представление");
			ИсходящиеДанные.ДобавитьЗначение("ТелефоныФизЛица","Вид");
			ИсходящиеДанные.ДобавитьЗначение("ФизическиеЛица","Справочник");
			ВыгрузитьПоПравилу(,, ИсходящиеДанные,, "КонтактнаяИнформация");
			Иначе
				ВыгрузитьТелефоны( ИсходящиеДанные, ВходящиеДанные, Источник, Приемник,,,,,,,,, ИмяПКО,,,,,,,,,,,,,,, УзелСсылки,,,,,, Отказ);
			КонецЕсли;
		КонецЕсли;
		
		Если ПустаяСтрока(Источник.ЮрАдрес) = 0 Тогда
			ИсходящиеДанные = СоздатьОбъект("СписокЗначений");
			ИсходящиеДанные.ДобавитьЗначение(Источник, "Объект");
			ИсходящиеДанные.ДобавитьЗначение("Адрес", "Тип");
			ИсходящиеДанные.ДобавитьЗначение(Источник.ЮрАдрес, "Представление");
			ИсходящиеДанные.ДобавитьЗначение("ЮрАдресФизЛица","Вид");
			ИсходящиеДанные.ДобавитьЗначение("ФизическиеЛица","Справочник");
			ВыгрузитьПоПравилу(,, ИсходящиеДанные,, "КонтактнаяИнформация");
		КонецЕсли;	
			
		Если ПустаяСтрока(Источник.ФактАдрес) = 0 Тогда	
			ИсходящиеДанные = СоздатьОбъект("СписокЗначений");
			ИсходящиеДанные.ДобавитьЗначение(Источник, "Объект");
			ИсходящиеДанные.ДобавитьЗначение("Адрес", "Тип");
			ИсходящиеДанные.ДобавитьЗначение(Источник.ФактАдрес, "Представление");
			ИсходящиеДанные.ДобавитьЗначение("ФактАдресФизЛица","Вид");
			ИсходящиеДанные.ДобавитьЗначение("ФизическиеЛица","Справочник");
			ВыгрузитьПоПравилу(,, ИсходящиеДанные,, "КонтактнаяИнформация");
		КонецЕсли;
		
		Если ПустаяСтрока(Источник.ДокументУдЛичность) = 0 Тогда
			Стр = Источник.ДокументУдЛичность;
			Стр1 = Стр;
			Стр = СтрЗаменить(Стр,"Иные документы, выдаваемые","Иные документы выдаваемые");
			Стр = СтрЗаменить(Стр,"Военный билет солдата (матроса, сержанта, старшины)","Военный билет солдата (матроса сержанта старшины)");
			Список = глРазложить(Стр);
			Если Найти(Стр1,"Иные документы, выдаваемые")>0 тогда 
				Список.УдалитьЗначение(1);
				Список.ВставитьЗначение(1,"Иные документы, выдаваемые органами МВД")
			КонецЕсли;
			Если Найти(Стр1,"Военный билет солдата")>0 тогда 
				Список.УдалитьЗначение(1);
				Список.ВставитьЗначение(1,"Военный билет солдата (матроса, сержанта, старшины)")
			КонецЕсли;	
			
			ДокументВид        = ?(0 < Список.РазмерСписка(), Список.ПолучитьЗначение(1), "");
			ДокументСерия      = ?(1 < Список.РазмерСписка(), Список.ПолучитьЗначение(2), "");
			ДокументНомер      = ?(2 < Список.РазмерСписка(), Список.ПолучитьЗначение(3), "");
			ДокументДатаВыдачи = ?(3 < Список.РазмерСписка(), Список.ПолучитьЗначение(4), "");
			ДокументКемВыдан   = ?(4 < Список.РазмерСписка(), Список.ПолучитьЗначение(5), "");
		
			ИсходящиеДанные = СоздатьОбъект("СписокЗначений");
			ИсходящиеДанные.ДобавитьЗначение(ДокументВид, "ДокументВид");
			ИсходящиеДанные.ДобавитьЗначение(ПолучитьДатуV8(Дата(ДокументДатаВыдачи)), "ДокументДатаВыдачи");
			ИсходящиеДанные.ДобавитьЗначение(ДокументКемВыдан, "ДокументКемВыдан");
			ИсходящиеДанные.ДобавитьЗначение(ДокументНомер,"ДокументНомер");
			ИсходящиеДанные.ДобавитьЗначение(ДокументСерия,"ДокументСерия");
			ИсходящиеДанные.ДобавитьЗначение(Источник,"ФизЛицо");
			ВыгрузитьПоПравилу(,, ИсходящиеДанные,, "ПаспортныеДанныеФизЛиц");
		КонецЕсли;

	Исключение
	
		Сообщить("Ошибка исполнения обработчика:  ПКО_ПослеВыгрузки_ФизическиеЛица", "!!!");
		Сообщить("  - " + ОписаниеОшибки());
		ЗафиксироватьОшибку("Ошибка исполнения обработчика:  ПКО_ПослеВыгрузки_ФизическиеЛица" + ": " + ОписаниеОшибки());

	КонецПопытки;



	Возврат Отказ;


КонецФункции // ПКО_ПослеВыгрузки_ФизическиеЛица()

