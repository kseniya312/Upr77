				Иначе
					НеПереноситьНомерИДатуСертификата = 1;
				КонецЕсли;	
			КонецЕсли;
			
			Если ((ПустоеЗначение(Источник.ГТД) = 1) И
				 (ПустоеЗначение(Источник.СтранаПроисхождения) = 1) И
				 ((НеПереноситьСерийныйНомер = 1) И (НеПереноситьНомерИДатуСертификата = 1))) Тогда
				 Отказ = 1;
			КонецЕсли;	
		КонецЕсли;

	Исключение
	
