Функция Конвертация_ПередВыгрузкойОбъекта(
			ИмяПКО, Правило, Объект, ИсходящиеДанные, ВходящиеДанные)


	Отказ = 0;


	Попытка


				
				Если ТипЗначенияСтр(Объект) = "Справочник" Тогда
					НеЗамещатьОбъектПриЗагрузке = Параметры.НеЗамещатьСправочникиПриЗагрузке;
				ИначеЕсли ТипЗначенияСтр(Объект) = "Документ" Тогда	
					НеЗамещатьОбъектПриЗагрузке = Параметры.НеЗамещатьДокументыПриЗагрузке;
					Если Параметры.ВыгружатьВБазовую = 1 Тогда
						Если Объект.ЮрЛицо <> Параметры.ОрганизацияПоУмолчанию Тогда
				        	Отказ = 1;
						КонецЕсли;
					КонецЕсли;
					Если Параметры.ПереноситьТолькоПроведенныеДокументы = 1 Тогда
						Если Объект.Проведен() = 0 Тогда
							Отказ = 1;
						КонецЕсли;
					КонецЕсли; 
				ИначеЕсли ТипЗначенияСтр(Объект) = "ТаблицаЗначений" Тогда
					Если Параметры.ВыгружатьВБазовую = 1 Тогда
		        		Если Объект.ЮрЛицо <> Параметры.ОрганизацияПоУмолчанию Тогда
				        	Отказ = 1;
						КонецЕсли;
					КонецЕсли;
				КонецЕсли;

	Исключение
	
		Сообщить("Ошибка исполнения обработчика:  Конвертация_ПередВыгрузкойОбъекта", "!!!");
		Сообщить("  - " + ОписаниеОшибки());
		ЗафиксироватьОшибку("Ошибка исполнения обработчика:  Конвертация_ПередВыгрузкойОбъекта" + ": " + ОписаниеОшибки());

